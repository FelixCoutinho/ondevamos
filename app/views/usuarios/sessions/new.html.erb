<h2>Entrar</h2>

<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
<fieldset>
  <div class="clearfix">
    <%= f.label :email %>
    <div class="input">
    <%= f.email_field :email %>
    </div>
  </div>
  <div class="clearfix">
    <%= f.label :password %>
    <div class="input">
    <%= f.password_field :password %>
    </div>
  </div>
  <div class="actions">
    <%= f.submit 'Entrar', :class => 'btn primary' %>
  </div>
</fieldset>
<% end %>

<%= render :partial => "usuarios/shared/links" %>
